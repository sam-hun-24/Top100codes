   "source": [
    "low, high = 150, 1000\n",
    "for i in range(low, high+1):\n",
    "    order = len(str(i))\n",
    "    sum = 0\n",
    "    temp = i\n",
    "    while temp>0:\n",
    "        digit = temp % 10\n",
    "        sum += digit**order\n",
    "        temp //= 10\n",
    "    if i == sum:\n",
    "        print(i, end=\", \")"
   ]
