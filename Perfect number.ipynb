   "source": [
    "n = 28\n",
    "sum = 0\n",
    "for i in range(1, n):\n",
    "    if n%i == 0:\n",
    "        sum = sum + i\n",
    "if sum == n:\n",
    "    print('Perfect number')\n",
    "else:\n",
    "    print('Not a perfect number')"
   ]
