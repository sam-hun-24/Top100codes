   "source": [
    "def prime(n, i=2):\n",
    "    if (n==i):\n",
    "        return True\n",
    "    elif (n%i==0):\n",
    "        return False\n",
    "    return prime(n, i+1)\n",
    "n = 971\n",
    "if prime(n):\n",
    "    print('Prime number')\n",
    "else:\n",
    "    print('Not a prime number')"
   ]
