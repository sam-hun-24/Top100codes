    "def rankArr(input1):\n",
    "    new = input1.copy()\n",
    "    new.sort()\n",
    "    for i in range(len(input1)):\n",
    "        for j in range(len(new)):\n",
    "            if input1[i] == new[j]:\n",
    "                input1[i] = j+1\n",
    "                break\n",
    "arr = [100, 2, 70, 12 , 90]\n",
    "rankArr(arr)\n",
    "print(arr)"
   ]
