    "def reverse_digit(arr, l):\n",
    "    for i in range(l):\n",
    "        arr[i], arr[-1*(i+1)] = arr[-1*(i+1)], arr[i]\n",
    "    return arr\n",
    "num = 12345\n",
    "arr = list(str(num))\n",
    "arr = reverse_digit(arr, len(arr)//2)\n",
    "s = ''\n",
    "print(int(s.join(arr)))"
   ]
