   "source": [
    "def printOrder(l, n):\n",
    "    l.sort()\n",
    "    for i in range(n//2):\n",
    "        print(l[i], end=' ')\n",
    "    for j in range(n-1, (n//2)-1, -1):\n",
    "        print(l[j], end=' ')\n",
    "l = [5, 4, 6, 2, 1, 3, 8, 9, 7]\n",
    "n = len(l)\n",
    "printOrder(l, n)"
   ]
